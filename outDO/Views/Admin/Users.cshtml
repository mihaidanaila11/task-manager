@{
}
<body>
	
	<div>
		<h2 class="display-2">USER MANAGEMENT</h2>
		<a class="btn btn-success mb-3" asp-controller="Admin" asp-action="GoBack">Go Back</a>
		@foreach(var user in ViewBag.users)
		{
			<div class="card mt-3">
				<div class="card-body">
					<h5>@user.UserName</h5>
					<p>@user.Email</p>
				</div>
			</div>
		}
		
	</div>
</body>